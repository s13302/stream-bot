{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "ignorePatterns": [
        "**/dist/*.js"
    ],
    "rules": {
        "no-console": "off",
        "max-len": "off",
        "import/extensions": "off",
        "import/no-unresolved": "off"
    }
}
